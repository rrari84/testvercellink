{
"builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/spa"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/spa/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/dist/spa/index.html"
    }
  ]
}